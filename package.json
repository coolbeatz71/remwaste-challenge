{
    "name": "remwaste-challenge",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "type:check": "tsc",
        "lint:code": "biome lint --no-errors-on-unmatched ./src",
        "lint:code:fix": "biome check --write ./src",
        "lint:types": "tsc --noEmit",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
        "prepare": "husky"
    },
    "dependencies": {
        "@biomejs/biome": "^1.9.4",
        "next": "15.2.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "enforce-branch-name": "^1.1.2",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
}
